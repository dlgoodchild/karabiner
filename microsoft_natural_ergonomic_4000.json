{
  "title": "Microsoft Natural Ergonomic Keyboard 4000 (model no. 1048 / KU-0462)",
  "rules": [
    {
      "description": "Swap left_command and left_control",
      "manipulators": [
        {
          "conditions": [
            {"type": "device_if", "identifiers": [{"vendor_id": 1118, "product_id": 219}]}
          ],
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": [
                "caps_lock"
                "left_command"
                "left_shift",
                "left_control"
                "right_shift",
                "right_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ]
        },
        {
          "conditions": [
            {"type": "device_if", "identifiers": [{"vendor_id": 1118, "product_id": 219}]}
          ],
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "caps_lock"
                "left_option"
                "left_shift",
                "left_control"
                "right_shift",
                "right_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_option"
            }
          ]
        }
      ]
    },
    {
      "description": "Swap non_us_backslash(ยง) and backslash(\\)",
      "manipulators": [
        {
          "conditions": [
            {"type": "device_if", "identifiers": [{"vendor_id": 1118, "product_id": 219}]}
          ],
          "type": "basic",
          "from": {
            "key_code": "non_us_backslash",
            "modifiers": {
              "optional": [
                "left_shift",
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ]
        }
      ]
    }
  ]
}
